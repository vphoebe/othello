{
  "name": "othello",
  "version": "1.0.0",
  "description": "game",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "esbuild src/index.ts --outdir=build --platform=node --target=node18 --bundle --packages=external --out-extension:.js=.cjs --minify",
    "dev": "nodemon src/index.ts",
    "deploy": "nodemon src/deploy.ts",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "author": "vphoebe",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.12.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "esbuild": "^0.17.18",
    "eslint": "^8.15.0",
    "eslint-plugin-require-extensions": "^0.1.3",
    "nodemon": "^3.0.1",
    "prettier": "2.6.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "prettier": {
    "semi": true
  }
}
